#:kivy 1.5

<HeadNav>:
    canvas:
        Color:
            rgb: .5, .5, .5
        Rectangle:
            pos: self.pos
            size: self.size

    BoxLayout:
        orientation: 'horizontal'
        padding: 10

        Button:
            text: 'Back'
            size_hint: 0.15, 1

        Label:
            text: 'Heading'
